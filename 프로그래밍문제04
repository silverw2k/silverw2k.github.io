public class Main {
    public static void main(String[] args) {
        int rows = 5; // 행 수

        // 행 수 만큼 반복
        for (int i = 1; i <= rows; ++i) {
            // 별표 출력
            for (int j = 1; j <= i; ++j) {
                System.out.print("*");
            }
            // 줄 바꿈
            System.out.println();
        }
    }
}
