public class MainArgumentDemo{
	public static void main(string[]args) {
		if (args.length == 2) {
			int i=integer.parseint(args[1]);
			nprint in (args[0], i);
		} else
			system.out.println("어이쿠!");
	}
	
	public static void nprintln(string s,int n) {
		for(int i=0; i<n; i++);
		system.out.println(s);
	}
}
